<header class="page-header">
  <h1>Log In</h1>
</header>
<div class="row">
  <div class="col-sm-6 col-sm-offset-1">
    <%= form_tag '/login' do %>
    Email: 
    <br>
      <%= text_field_tag :email%>
    <br>
    Password: 
    <br>
      <%= password_field_tag :password %>
    <br>
    <br>
    <%= submit_tag 'Submit' %> 
    <% end %>
  </div>
</div>
